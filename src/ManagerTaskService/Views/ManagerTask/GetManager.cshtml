@model ManagerTask.Data.Manager

@{
    ViewBag.Title = "Manager Info";
}

<h2>Manager</h2>

@if (ViewBag.Error != null)
{
    <p class="text-danger">@ViewBag.Error</p>
}
else
{
    if (Model != null)
    {
        <div>
            <label>Name</label> @Model.Name
        </div>
    }
    else
    {
        <p class="text-danger">No manager found</p>
    }
}

